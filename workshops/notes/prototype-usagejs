const person1 = {
  x: 1,
  y: 2,
  add: () => this.x + this.y
};

const person2 = {
  x: 4,
  y: 7,
  add: () => this.x + this.y
};

class Person {
  constructor(x, y) {
    this.x = x;
    this.y = y;
  }

  add() {
    return this.x + this.y;
  }
}

const person1 = new Person(1, 2);
const person2 = new Person(3, 4);
